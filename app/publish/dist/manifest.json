{
  "_version": "1.49.0",
  "sap.app": {
    "id": "sample-cap-app.publish",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "type": "application",
    "title": "{{Browse Publishers}}",
    "description": "{{appDescription}}",
    "i18n": {
      "bundleUrl": "i18n/i18n.properties",
      "supportedLocales": [
        "",
        "de",
        "en"
      ]
    },
    "dataSources": {
      "CatalogService": {
        "uri": "odata/v4/catalog/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "Publishers-display": {
          "signature": {
            "parameters": {
              "Publishers.ID": {
                "renameTo": "ID"
              }
            },
            "additionalParameters": "allowed"
          },
          "semanticObject": "Publishers",
          "action": "display",
          "title": "{{Browse Publishers}}",
          "info": "{{appInfo}}",
          "subTitle": "{{appSubTitle}}",
          "icon": "sap-icon://course-book",
          "indicatorDataSource": {
            "dataSource": "CatalogService",
            "path": "Publishers/$count",
            "refresh": 1800
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "fullWidth": false,
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "dependencies": {
      "minUI5Version": "1.115.1",
      "libs": {
        "sap.fe.templates": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties",
        "settings": {
          "supportedLocales": [
            "",
            "de",
            "en"
          ]
        }
      },
      "": {
        "dataSource": "CatalogService",
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true,
          "groupProperties": {
            "default": {
              "submit": "Auto"
            }
          }
        }
      }
    },
    "routing": {
      "routes": [
        {
          "pattern": ":?query:",
          "name": "PublishersList",
          "target": "PublishersList"
        },
        {
          "pattern": "Publishers({key}):?query:",
          "name": "PublishersDetails",
          "target": "PublishersDetails"
        }
      ],
      "targets": {
        "PublishersList": {
          "type": "Component",
          "id": "PublishersList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "entitySet": "Publishers",
              "initialLoad": true,
              "navigation": {
                "Publishers": {
                  "detail": {
                    "route": "PublishersDetails"
                  }
                }
              }
            }
          }
        },
        "PublishersDetails": {
          "type": "Component",
          "id": "PublishersDetailsList",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "entitySet": "Publishers"
            }
          }
        }
      },
      "config": {}
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  },
  "sap.cloud": {
    "public": true,
    "service": "samplecapapp.service"
  }
}