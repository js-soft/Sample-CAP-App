{
  "_version": "1.49.0",
  "sap.app": {
    "id": "sample-cap-app.browse-sales-orders",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "type": "application",
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "i18n": "i18n/i18n.properties",
    "dataSources": {
      "SalesService": {
        "uri": "odata/v4/sales/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      },
      "SalesServiceAnnotations": {
        "type": "ODataAnnotation",
        "uri": "annotations/annotations.xml",
        "settings": {
          "odataVersion": "4.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "SalesOrders-display": {
          "signature": {
            "parameters": {
              "SalesOrders.ID": {
                "renameTo": "ID"
              }
            },
            "additionalParameters": "allowed"
          },
          "semanticObject": "SalesOrders",
          "action": "display",
          "title": "{{appTitle}}",
          "info": "{{appInfo}}",
          "subTitle": "{{appSubTitle}}",
          "indicatorDataSource": {
            "dataSource": "SalesService",
            "path": "SalesOrders/$count",
            "refresh": 1800
          }
        }
      },
      "outbounds": {
        "Books-display": {
          "semanticObject": "Books",
          "action": "display",
          "parameters": {
            "Books.ID": {}
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "fullWidth": false,
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "dependencies": {
      "minUI5Version": "1.115.1",
      "libs": {
        "sap.fe.templates": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      },
      "": {
        "dataSource": "SalesService",
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true,
          "groupProperties": {
            "default": {
              "submit": "Auto"
            }
          }
        }
      }
    },
    "routing": {
      "routes": [
        {
          "pattern": ":?query:",
          "name": "SalesOrdersList",
          "target": "SalesOrdersList"
        },
        {
          "pattern": "SalesOrders({key}):?query:",
          "name": "SalesOrdersDetails",
          "target": "SalesOrdersDetails"
        }
      ],
      "targets": {
        "SalesOrdersList": {
          "type": "Component",
          "id": "SalesOrdersList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "entitySet": "SalesOrders",
              "initialLoad": true,
              "navigation": {
                "SalesOrders": {
                  "detail": {
                    "route": "SalesOrdersDetails"
                  }
                }
              }
            }
          }
        },
        "SalesOrdersDetails": {
          "type": "Component",
          "id": "SalesOrdersDetailsList",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "entitySet": "SalesOrders",
              "showRelatedApps": true
              
            }
          }
        }
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  },
  "sap.cloud": {
    "public": true,
    "service": "samplecapapp.service"
  }
}
