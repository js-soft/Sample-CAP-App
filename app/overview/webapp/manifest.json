{
  "sap.app": {
    "id": "overview",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/analytics/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui5": {
    "models": {
      "": {
        "dataSource": "mainService",
        "preload": true
      }
    }
  },
  "sap.ovp": {
    "globalFilterModel": "",
    "globalFilterEntityType": "",
    "cards": {
      "byGenre": {
        "model": "",
        "template": "sap.ovp.cards.charts.analytical",
        "settings": {
          "entitySet": "OrdersByGenre",
          "annotationPath": "com.sap.vocabularies.UI.v1.Chart#ByGenre",
          "title": "{{ByGenre}}"
        }
      },
      "revenueDaily": {
        "model": "",
        "template": "sap.ovp.cards.charts.analytical",
        "settings": {
          "entitySet": "RevenueByDay",
          "annotationPath": "com.sap.vocabularies.UI.v1.Chart#RevenueDaily",
          "title": "{{RevenueByDay}}"
        }
      },
      "topBooks": {
        "model": "",
        "template": "sap.ovp.cards.charts.analytical",
        "settings": {
          "entitySet": "TopBooks",
          "annotationPath": "com.sap.vocabularies.UI.v1.Chart#TopBooks",
          "title": "{{TopBooks}}"
        }
      },
      "topCustomers": {
        "model": "",
        "template": "sap.ovp.cards.charts.analytical",
        "settings": {
          "entitySet": "TopCustomers",
          "annotationPath": "com.sap.vocabularies.UI.v1.Chart#TopCustomers",
          "title": "{{TopCustomers}}"
        }
      }
    },
    "chartSettings": {}
  }
}
