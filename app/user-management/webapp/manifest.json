{
  "_version": "1.49.0",
  "sap.app": {
    "id": "sample-cap-app.user-management",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "type": "application",
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "i18n": "i18n/i18n.properties",
    "dataSources": {
      "UserService": {
        "uri": "odata/v4/user/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "intent-Users-manage": {
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          },
          "semanticObject": "Users",
          "action": "manage"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "fullWidth": false,
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "dependencies": {
      "minUI5Version": "1.115.1",
      "libs": {
        "sap.fe.templates": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      },
      "": {
        "dataSource": "UserService",
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true,
          "groupProperties": {
            "default": {
              "submit": "Auto"
            }
          }
        }
      }
    },
    "routing": {
      "routes": [
        {
          "pattern": ":?query:",
          "name": "UsersList",
          "target": "UsersList"
        },
        {
          "pattern": "Users({key}):?query:",
          "name": "UsersDetails",
          "target": "UsersDetails"
        }
      ],
      "targets": {
        "UsersList": {
          "type": "Component",
          "id": "UsersList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "entitySet": "Users",
              "initialLoad": true,
              "variantManagement": "Page",
              "navigation": {
                "Users": {
                  "detail": {
                    "route": "UsersDetails"
                  }
                }
              }
            }
          }
        },
        "UsersDetails": {
          "type": "Component",
          "id": "UsersDetails",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "entitySet": "Users",
              "editableHeaderContent": false,
              "variantManagement": "Page"
            }
          }
        }
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  },
  "sap.cloud": {
    "public": true,
    "service": "samplecapapp.service"
  }
}
